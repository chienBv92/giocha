@using Web_Gio_Cha.Resources;
@using Web_Gio_Cha.Models.Define;
@using Web_Gio_Cha.EF;

@model IEnumerable<Slide>
<div class="slideLeft" style="width:100%">
    @if (Model.Count() > 0)
    {
        foreach (var item in Model)
        {
            <div class="item">
                <a href="@item.Link" target="_blank"> <img src="@item.Image" alt="giò chả Vân Bảo" style="width:100%;"></a>
            </div>
        }
    }

</div>

